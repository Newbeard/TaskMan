<script defer src='/js/boardOne.js'></script>
<script defer src='/js/chat.js'></script>
 <link rel="stylesheet" href="/css/styles.css" />

    <div>
      <div class="row">
          <div class="col-header start">Начать</div>
          <div class="col-header progress">В процессе</div>
          <div class="col-header done">Готовы</div>
      </div>

      <div class="row1">
        <div id="startPlaceholder" class="placeholder" data-statusTask="1">
          {{#each taskStart}}
           {{>statusTaskStart}}
             {{/each}}
        </div>

        <div class="placeholder" data-statusTask="2">
         {{#each taskProcess}}
            {{>statusTaskProcess}}
           {{/each}}
        </div>

        <div class="placeholder" data-statusTask="3">
            {{#each taskEnd}}
           {{>statusTaskEnd}}
             {{/each}}

        </div>
      </div>
      
    </div>
    


